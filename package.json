    "dev": "concurrently 'next dev' 'yarn dev:generate:hygraph-types'",
    "dev:sync-env": "vercel env pull",
    "build": "yarn generate:hygraph-types && yarn generate:resume && next build",
    "dev:build": "env-cmd -f .env.local yarn build",
    "start": "next start",
    "lint": "next lint",
    "generate:resume": "npx tsx src/resume/generate.tsx",
    "dev:generate:resume": "env-cmd -f .env.local yarn generate:resume",
    "generate:hygraph-types": "graphql-codegen --config src/hygraph/codegen.ts",
    "dev:generate:hygraph-types": "env-cmd -f .env.local yarn generate:hygraph-types --watch",
    "deploy:staging": "git push origin main:staging --force && git push origin main:staging-preview --force",
    "deploy:production": "git push origin main:production --force && git push origin main:production-preview --force",
    "script:migrate-color-sets": "env-cmd -f .env.local npx tsx scripts/migrate-color-sets.ts"
